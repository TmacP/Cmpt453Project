function init(self)
	msg.post("#proxy_level_4", "load")                      -- [1]
	msg.post(".", "acquire_input_focus")                -- [1]
end

function on_message(self, message_id, message, sender)
	if message_id == hash("proxy_loaded") then              -- [2]
		msg.post(sender, "init")
		msg.post(sender, "enable")
	end
end